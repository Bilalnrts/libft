int main()
{
    printf("%d a :\n", ft_isalpha('a'));
    printf("%d z :\n", ft_isalpha('z'));
    printf("%d A :\n", ft_isalpha('A'));
    printf("%d Z :\n", ft_isalpha('Z'));
    printf("%d a :\n", ft_isalpha(97));
    printf("%d z :\n", ft_isalpha(122));
    printf("%d A :\n", ft_isalpha(65));
    printf("%d Z :\n", ft_isalpha(90));
    printf("%d Z :\n", ft_isalpha(32));
    return 0;
}
